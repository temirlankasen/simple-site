{
    "name": "perfumerlabs/site",
    "type": "library",
    "description": "Docker container for building personal sites",
    "homepage": "https://perfumerlabs.com",
    "authors": [
        {
            "name": "Ilyas Makashev",
            "email": "mehmatovec@gmail.com",
            "role": "Maintainer"
        },
        {
            "name": "Temirlan Kasen",
            "email": "temirlankasen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "7.4.*",
        "perfumer/framework": "0.9.2",
        "propel/propel": "2.0.0-alpha11",
        "envms/fluentpdo": "2.*",
        "pwfisher/command-line-php": "dev-master",
        "symfony/console": "5.*",
        "symfony/http-foundation": "5.*",
        "nikic/fast-route": "1.*",
        "guzzlehttp/guzzle": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "sl4mmer/phpcent": "v2.0.0",
        "twig/twig": "1.42.5"
    },
    "require-dev": {
        "filp/whoops": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Project\\": "src/",
            "Site\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "platform":{
            "php":"7.4"
        }
    }
}
